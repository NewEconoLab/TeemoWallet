{
    "manifest_version": 2,

    "name": "Teemo Wallet",
    "description":
        "A boilerplate project to quickly build a Chrome extension using TypeScript and React (built using webpack).",
    "version": "1.0.0",

    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "popup.html"
    },

    "background": {
        "scripts": [
            "lib/neo-thinsdk.min.js",
            "js/background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}
